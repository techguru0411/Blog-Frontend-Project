@import "theme.styl";

#article-page-content
  > article
    text-align left
    padding 0 10px
    pre
      font-size 16px
      line-height 20px
      display block
      white-space pre-wrap
      color $color-typegraphy-title
      font-weight 200
    p
      font-size 17px
      line-height 25px
      color $color-typegraphy-title
      font-weight 200
    img
      width 100%